{
    "rules_file_name": "./magic_sturdy/magic.txt",
    "sequence_token_symbols": {
        "★": "*",
        "☆": "@"
    },
    "wordbreak_symbol": {
        "⎵": "^"
    },
    "token_aliases": {
        "✤": "q",
        "✪": "l"
    },
    "output_func_symbols": "↻⇑",
    "comment_str": "//",
    "separator_str": "⇒",
    "implicit_transform_leading_wordbreak": true
}
